{
    "name": "Dakiya Api Server",
    "version": "1.0.0",
    "description": "This application is Backend Api server For Dakiya",
    "private": true,
    "scripts": {
        "link": "rm -rf node_modules && ln -s ../node_modules node_modules",
        "build": "rm -rf dist && tsc",
        "build:prod": "rm -rf dist && tsc --project ./tsconfig-prod.json",
        "webpack": "rm -rf dist && webpack",
        "webpack:prod": "rm -rf dist && webpack --mode=production",
        "start": "npm run webpack && node ./dist",
        "start:debug": "node ../bin/runner.js --server --debug",
        "start:dev": "node --inspect ../bin/runner.js --server",
        "test": "echo \"Error: no test specified\" && exit 1"
    }
}